openapi: 3.0.2
info:
  title: ACAPS API
  version: v1
  description: <p>We are an independent information provider specialised in humanitarian
    needs analysis.</p><p>You can find detailed information on using ACAPS API <a
    href='/using-the-api/'>here</a>, including authentication, making requests, and
    filtering and ordering the response. Working examples in the Python and R programming
    languages can be found <a href='/working-examples/'>here</a>.</p><p>If you're
    interested in discussing collaborations with ACAPS or use of the API, or have
    any technical queries relating to the API, please <a href='/contact-us'>contact
    us</a>.</p>
paths:
  /api/v1/crises/:
    get:
      operationId: listCrisis
      description: This dataset is a central list of the crises monitored by ACAPS.<p
        class='dataset-link'>Explore the dataset [here](/api/v1/crises/).</p>
      parameters:
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: crisis_id
        required: false
        in: query
        description: crisis_id
        schema:
          type: string
      - name: crisis_name
        required: false
        in: query
        description: crisis_name
        schema:
          type: string
      - name: website_name
        required: false
        in: query
