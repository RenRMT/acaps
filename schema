        required: false
        in: query
        description: individual_aggregated
        schema:
          type: string
      - name: active
        required: false
        in: query
        description: active
        schema:
          type: string
      - name: geoscope
        required: false
        in: query
        description: geoscope
        schema:
          type: string
      - name: connected_crises
        required: false
        in: query
        description: connected_crises
        schema:
          type: string
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      responses:
        '200':
          content:
            application/json:
              schema: &id001
                type: object
                properties:
                  count:
                    type: integer
                    example: 123
                  next:
                    type: string
                    nullable: true
                  previous:
                    type: string
                    nullable: true
                  results:
                    type: array
                    items:
                      properties:
                        crisis_id:
                          type: string
                        crisis_name:
                          type: string
