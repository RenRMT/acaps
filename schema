  /api/v1/inform-severity-index/reliability/updated-days/{date}/:
    get:
      operationId: RetrieveInformSeverityIndexLog
      description: 'The INFORM Severity Index, formerly known as the Global Crisis
        Severity Index (GCSI), is an improved, timely way of measuring the severity
        of humanitarian crises and disasters globally. The INFORM Severity Index is
        a sensitive, regularly updated, and easily interpreted model for measuring
        crisis severity that can assist decision-makers and contribute to improved
        effectiveness and coordination in humanitarian action.


        This dataset calculates reliability scores for the crises, calculated using
        the number of days since indicators were last updated.<p class=''dataset-link''>Explore
        the dataset [here](/api/v1/inform-severity-index/reliability/updated-days/).</p>'
      parameters:
      - name: date
        in: path
        required: true
        description: Month and year to calculate Inform Severity results for, e.g.
          to calculate Inform Severity results for January 2020, enter 'Jan2020'.
          Enter any date in the range 'Jan2019' to current.
        schema:
          type: string
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      - name: crisis_id
        required: false
        in: query
        description: ID of the crisis e.g. AFG001
        schema:
          type: string
      - name: crisis_name
        required: false
        in: query
        description: Name of the crisis
        schema:
          type: string
      - name: type_of_crisis
        required: false
        in: query
        description: Type of crisis, e.g. 'Complex crisis'
        schema:
          type: string
      - name: regional_or_country
        required: false
        in: query
        description: 'Whether the crisis is country wide or regional, options: ''Country''
          or ''Regional'''
        schema:
          type: string
      - name: website_name
        required: false
        in: query
        description: Website name of the crisis
        schema:
          type: string
      - name: iso3
        required: false
        in: query
        description: ISO3 code, e.g. AFG
        schema:
          type: string
      - name: country
        required: false
        in: query
        description: Country name, e.g. 'Afghanistan'
        schema:
          type: string
      - name: regions
        required: false
        in: query
        description: Region names, e.g. 'Asia'
        schema:
          type: string
      - name: country_level
        required: false
        in: query
        description: When there is only one crisis in the country, the crisis provides
          a score for the country level. If there are multiple crisis within a country,
          the country level crisis is an aggregation of the impact of the different
          situations at country level.
        schema:
          type: string
      - name: individual_aggregated
        required: false
        in: query
        description: Indivudiaul crises are scores specific to one crisis, whereas
          aggregated crises consider all active crises present in a country.
        schema:
          type: string
      responses:
        '200':
          content:
            application/json:
              schema: &id038
                properties:
                  id:
                    type: integer
                    maximum: 9223372036854775807
                    minimum: -9223372036854775808
                    format: int64
                  crisis_id:
                    type: string
                    nullable: true
                  crisis:
                    type: string
                    nullable: true
                  country:
                    type: array
                    items:
                      type: string
                    nullable: true
                  iso3:
                    type: array
                    items:
                      type: string
                    nullable: true
                  indicator:
                    type: string
                    nullable: true
                  figure:
                    type: number
                    nullable: true
                  source_and_date:
                    type: string
                    nullable: true
                  reliability:
                    type: string
                    nullable: true
                  reliability_score:
                    type: number
                    nullable: true
                  link:
                    type: string
                    nullable: true
                  justification:
                    type: string
                    nullable: true
                  date_of_entry:
                    type: string
                    format: date
                    nullable: true
                  type_of_entry:
                    type: string
                    nullable: true
                  _internal_filter_date:
                    type: string
                    format: date
                    nullable: true
                required:
                - id
